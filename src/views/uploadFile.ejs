<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/styles.css" />
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css" />
    <title>NodejsBasic</title>
  </head>
  <body>
    <div class="topnav">
      <a href="/">Home</a>
      <a href="/upload" class="active">UploadFile</a>
    </div>
    <div class="upload-file-container">
      <form
        action="/upload-profile-pic"
        method="POST"
        enctype="multipart/form-data"
      >
        <div>
          <label for="">Picture:</label>
          <input type="file" name="profile_pic" />
        </div>
        <div><input type="submit" class="btn-save" id="" />Save</div>
      </form>
      <form
        action="/upload-multiple-pic"
        method="POST"
        enctype="multipart/form-data"
      >
        <div>
          <label for="">Pictures:</label>
          <input type="file" name="profile_pic_mutiple" multiple />
        </div>
        <div><input type="submit" class="btn-save" id="" />Save</div>
      </form>
    </div>
  </body>
</html>
